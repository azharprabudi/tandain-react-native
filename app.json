{
  "expo": {
    "name": "Tandain",
    "description": "A very interesting project.",
    "slug": "Tandain",
    "privacy": "public",
    "sdkVersion": "28.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.mobile.tandain",
      "config": {
        "googleMaps": { "apiKey": "{YOUR_API_KEY_GOOGLE_MAPS}" }   
      }
    }
  }
}
